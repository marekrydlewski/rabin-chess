{
  "name": "rabin-chess",
  "version": "1.0.0",
  "description": "Rabin Chess - chess app built in react",
  "author": "Marek Rydlewski Marcin Jabloński Konrad Matusiak Michał Panowicz",
  "license": "MIT",
  "config": {
    "port": 9000
  },
  "scripts": {
    "clean": "rimraf build/ *.log",
    "build": "npm run clean && webpack --config webpack/config.build.js && npm run copy",
    "dev": "npm run clean && npm run mkdir && npm run copy && webpack-dev-server --config webpack/config.dev.js",
    "test": "jest",
    "flow": "flow check --color always --show-all-errors",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint src/scripts/**/*",
    "lint:sass": "sass-lint -v src/styles/**/*",
    "docs": "esdoc -c esdoc.json",
    "copy": "npm run copy:chessboard",
    "copy:chessboard": "npm run copy:chessboard:img && npm run copy:chessboard:css",
    "copy:chessboard:img": "ncp node_modules/chessboardjs/www/img/ build/img/",
    "copy:chessboard:css": "ncp node_modules/chessboardjs/www/css/ build/css/",
    "mkdir": "mkdirp build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcin-jablonski/rabin-chess.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-jest": "^11.0.0",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.0",
    "css-loader": "^0.23.0",
    "esdoc": "^0.4.6",
    "esdoc-es7-plugin": "^0.0.3",
    "eslint": "^2.8.0",
    "eslint-plugin-flowtype": "^2.2.6",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.0",
    "flow-bin": "^0.23.0",
    "html-webpack-plugin": "^2.16.0",
    "jest-cli": "^11.0.0",
    "json-loader": "^0.5.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "3.4.2",
    "postcss-loader": "^0.8.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.0",
    "sass-lint": "^1.5.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.12.0"
  },
  "dependencies": {
    "chess.js": "^0.9.4",
    "chessboardjs": "0.0.1",
    "classnames": "^2.2.3",
    "core-js": "^2.2.2",
    "flexboxgrid": "^6.3.0",
    "normalize.css": "^4.1.1",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-favicon": "0.0.4",
    "react-flexbox-grid": "^0.9.6",
    "react-redux": "^4.4.5",
    "react-router": "^2.1.1",
    "react-slick": "^0.12.0",
    "react-toolbox": "^0.16.2",
    "redux": "^3.5.1",
    "redux-thunk": "^2.1.0"
  }
}
